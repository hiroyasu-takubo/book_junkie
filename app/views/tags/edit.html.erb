<% provide(:title, "タグの編集") %>
<h1>タグの編集</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@tags) do |f| %>
      <%= render 'shared/error_messages' , f.object%>

      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>
      
      <%= f.submit "保存", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
