<% provide(:title, 'すべてのタグ') %>
<h1>タグ一覧</h1>

<div class="row">
  <ul class="tags">
    <% @tags.each do |tag| %>
      <li>
        <p><%= tag.name %>
        <div class="right">
          <%= link_to "削除", tag, method: :delete, data: { confirm: "削除をしますか？" }  %>
        </div>
        </p>
      </li>
    <% end %>
  </ul>
</div>
